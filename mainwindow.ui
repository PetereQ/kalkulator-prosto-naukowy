  <property name="styleSheet">
   <string notr="true">/* Tło całej aplikacji - ciemnoszare */
QMainWindow {
    background-color: #2b2b2b;
}

/* Pola tekstowe - nieco jaśniejsze, biały tekst */
QLineEdit {
    background-color: #3c3f41;
    color: #ffffff;
    border: 1px solid #555;
    border-radius: 5px;
    padding: 5px;
    font-size: 16px;
    font-family: &quot;Segoe UI&quot;, sans-serif;
}

/* Przyciski - nowoczesny płaski styl */
QPushButton {
    background-color: #505050;
    color: #ffffff;
    border: 1px solid #3c3f41;
    border-radius: 6px;
    padding: 10px;
    font-size: 20px;
    font-weight: bold;
    min-width: 60px;
}

/* Efekt po najechaniu myszką */
QPushButton:hover {
    background-color: #606060;
    border-color: #707070;
}

/* Efekt wciśnięcia */
QPushButton:pressed {
    background-color: #4da6ff; /* Niebieski akcent przy kliknięciu */
    color: #ffffff;
}

/* Przycisk = - Zielony*/
QPushButton#equalsButton {
    color: #00ff7f;
    font-weight: bold;
}

/* Efekt najechania myszką dla = */
QPushButton#equalsButton:hover {
    background-color: rgba(0, 255, 127, 0.1); 
}

/*Efekt po klikneciu dla = */
QPushButton#equalsButton:pressed{
	background-color: #00ff7f;
	color: #ffffff;
}


/* Przycisk Delete - Czerwony*/
QPushButton#deleteButton {
    color: #ff5555; 
    font-weight: bold;
}

/* Przycisk Clear - Czerwony */
QPushButton#clearButton{ 
    color: #ff5555; 
    font-weight: bold;
}

/* Efekt najechania dla czerwonych */
QPushButton#deleteButton:hover, QPushButton#clearButton:hover{
    background-color: rgba(255, 85, 85, 0.1); 
}

/*Efekt po klikneciu dla czerwonych */
QPushButton#deleteButton:pressed, QPushButton#clearButton:pressed{
 	background-color: #ff5555; 
    color: #ffffff;
}</string>
