}

/* Przyciski - nowoczesny płaski styl */
QPushButton {
    background-color: #505050;
    color: #ffffff;
    border: 1px solid #3c3f41;
    border-radius: 6px;
    padding: 10px;
    font-size: 14px;
    font-weight: bold;
    min-width: 60px; /* Żeby nie były za wąskie */
}

/* Efekt po najechaniu myszką */
QPushButton:hover {
    background-color: #606060;
    border-color: #707070;
}

/* Efekt wciśnięcia */
QPushButton:pressed {
    background-color: #4da6ff; /* Niebieski akcent przy kliknięciu */
    color: #ffffff;
}

/* Wyróżnienie dla przycisku &quot;=&quot; lub głównego działania (opcjonalnie) */
QPushButton#runButton {
    background-color: #0078d7;
}</string>
